<div class="article-loop row container-fluid event-list">
    <h2 class="article-h1 h1-event-list col-md-5 col-12"><i class="fal fa-calendar"></i> Nous voir</h2>
    <div class="offset-md-7"></div>
    <section class="event container-fluid">
        {% for oneevent in oneevents %}

            {% for show in shows %}
                {% if show.get_field('dispo') == 'Spectacle disponible' %}


                    {% if show.get_field('field_5cbc31c7f2c13') == oneevent.get_field('field_5cbc31c7f2c13') %}
                        {% if "now"|date < oneevent.get_field('field_5cb4d81344d45')  %}


                                <a class="row event-home-bloc" href="{{ oneevent.link }}">
                                    <div class="offset-md-3 d-none d-md-block"></div>
                                    <div class="thumb thumb2 col-12 col-md-6 d-none d-md-block">
                                        <img src="{{ show.thumbnail.src }}" alt="thumb">
                                    </div>
                                    <div class="offset-md-3 d-none d-md-block"></div>
                                    <h2 class="article-h3 col-12 ">{{ show.title }}</h2>

                                    <p class="date-list col-12">{{ oneevent.get_field('field_5cb4d81344d45') }}</p>

                                    <p class="salle col-12">{{ oneevent.get_field('field_5cb4e5ba17780') }}</p>
                                    <p class="col-12">{{ oneevent.get_field('field_5cb4e62317783') }} {{ oneevent.get_field('field_5cb4e63d17784') }}</p>
                                </a>

                        {% endif %}
                    {% endif %}

                {% endif %}
            {% endfor %}
        {% endfor %}
    </section>

</div>